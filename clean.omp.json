{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#BF616A",
          "style": "plain",
          "type": "status",
          "template": "\uf52f {{.Code}} "
        },
        {
          "foreground": "#5FAAE8",
          "style": "plain",
          "template": "\u03BB",
          "type": "text"
        },
        {
          "foreground": "#56B6C2",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "foreground": "#CCCCCC",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": "<#5FAAE8>git:(</>{{ .HEAD }}<#5FAAE8>)</>",
          "type": "git"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
